{
  "name": "spider-byte",
  "module": "./src/index.ts",
  "type": "module",
  "devDependencies": {
    "@marcrock22/eslint": "github:marcrock22/eslintcf",
    "@types/bun": "^1.2.20",
    "eslint": "^9.34.0",
    "rimraf": "^6.0.1",
    "ts-patch": "^3.3.0",
    "typescript-eslint": "^8.40.0",
    "typia": "^9.7.1"
  },
  "peerDependencies": {
    "typescript": "~5.9.2"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.77",
    "@prisma/client": "^6.14.0",
    "@redis/client": "^5.8.2",
    "@resvg/resvg-js": "^2.6.2",
    "@top-gg/sdk": "^3.1.6",
    "cheerio": "^1.1.2",
    "didyoumean2": "^7.0.4",
    "domhandler": "^5.0.3",
    "prisma": "^6.14.0",
    "satori": "^0.16.2",
    "satori-html": "^0.3.2",
    "seyfert": "^3.2.6"
  },
  "scripts": {
    "start": "bun run build && bun --env-file=.env ./dist/index.ts",
    "prepare": "ts-patch install",
    "build": "bunx rimraf dist && typia generate --input src --output dist --project tsconfig.json",
