{
  "name": "spider-byte",
  "module": "./src/index.ts",
  "type": "module",
  "devDependencies": {
    "@marcrock22/eslint": "github:marcrock22/eslintcf",
    "@types/bun": "^1.3.9",
    "eslint": "^9.39.2",
    "rimraf": "^6.1.3",
    "ts-patch": "^3.3.0",
    "typescript-eslint": "^8.56.0",
    "typia": "^11.0.3"
  },
  "peerDependencies": {
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.92",
    "@prisma/client": "6.19",
    "@redis/client": "^5.11.0",
    "@resvg/resvg-js": "^2.6.2",
    "@top-gg/sdk": "^3.1.6",
    "cheerio": "^1.2.0",
    "didyoumean2": "^7.0.4",
