{
  "name": "spider-byte",
  "module": "./src/index.ts",
  "type": "module",
  "devDependencies": {
    "@marcrock22/eslint": "github:marcrock22/eslintcf",
    "@types/bun": "^1.2.2",
    "eslint": "^9.20.1",
    "rimraf": "^6.0.1",
    "ts-patch": "^3.3.0",
    "typescript-eslint": "^8.24.0",
    "typia": "^7.6.4"
  },
  "peerDependencies": {
    "typescript": "~5.7.3"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.67",
    "cheerio": "^1.0.0",
    "didyoumean2": "^7.0.4",
    "domhandler": "^5.0.3",
    "seyfert": "^2.2.1-dev-13340711253.0",
    "sharp": "^0.33.5"
  },
  "scripts": {
    "start": "bun run build && bun --env-file=.env ./dist/index.ts",
    "prepare": "ts-patch install && typia patch",
    "build": "bunx rimraf dist && typia generate --input src --output dist --project tsconfig.json",
    "production": "bun run build && bun --env-file=.env ./dist/index.ts production"
  }
}